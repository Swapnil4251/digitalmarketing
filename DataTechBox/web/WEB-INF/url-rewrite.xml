<?xml version="1.0" encoding="utf-8"?>

    <!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite>

	<rule>
		<from>/mail-interface</from>
		<to type="forward">/inspinia.xhtml</to>
	</rule>
	
	<rule>
		<from>/*</from>
		<to type="forward">/$1.xhtml</to>
	</rule>
</urlrewrite>